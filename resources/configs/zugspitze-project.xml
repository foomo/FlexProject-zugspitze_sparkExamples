<?xml version="1.0" encoding="utf-8" ?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.foomo.org/zugspitze" xsi:schemaLocation="http://www.foomo.org/zugspitze http://www.foomo.org/foomo/modules/Foomo.Zugspitze/schemas/project.xsd">
	<id>org.foomo.zugspitze.flex4.examples</id>
	<type>Implementation Project</type>
	<name>Flex 4 Examples</name>
	<description>Flex 4 Examples.</description>

	<dependencies>
		<id>org.foomo.flash</id>
		<id>org.foomo.zugspitze.core</id>
		<id>org.foomo.zugspitze.flex4</id>
	</dependencies>

	<applications>

		<application>
			<id>org.foomo.zugspitze.flex4.examples.applications.blank</id>
			<name>Blank Application</name>
			<description>This is a blank application skeleton.</description>
			<package>org.foomo.zugspitze.examples.applications.blank</package>
			<sources>
				<path>src/BlankApplication.mxml</path>
				<path>src/assets/css/Application.css</path>
				<path>src/org/foomo/zugspitze/examples/applications/blank</path>
			</sources>
		</application>

		<application>
			<id>org.foomo.zugspitze.flex4.examples.components.blank</id>
			<name>Blank Component</name>
			<description>This is a blank component skeleton.</description>
			<package>org.foomo.zugspitze.examples.components.blank</package>
			<sources>
				<path>src/org/foomo/zugspitze/examples/components/blank</path>
			</sources>
		</application>

		<application>
			<id>org.foomo.zugspitze.flex4.examples.components.logger</id>
			<name>Logger Component</name>
			<description>Example component to show how to use the logger api.</description>
			<package>org.foomo.zugspitze.examples.components.logger</package>
			<sources>
				<path>src/org/foomo/zugspitze/examples/components/logger</path>
			</sources>
		</application>

	</applications>
</config>